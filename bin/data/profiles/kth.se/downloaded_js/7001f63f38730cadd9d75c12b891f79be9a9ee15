<script>
  if (document.head) {
    const styleContainer = document.getElementById("widget_vlfwioyz")
    
    const cssStyle = styleContainer.getAttribute("data-style")

    if (cssStyle && cssStyle !== '') {
      const style = document.createElement('style')
      style.type = 'text/css'


      if (style.styleSheet) {
        style.styleSheet.cssText = styleContainer.getAttribute("data-style")
      } else {
        style.appendChild(document.createTextNode(styleContainer.getAttribute("data-style")))
      }

      document.head.appendChild(style)
    }
  } else {
    console.warn('Could not load style for widget due to missing head tag')
  }
